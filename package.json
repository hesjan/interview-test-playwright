{
  "name": "interview-test-playwright",
  "version": "1.0.0",
  "description": "End-to-end tests for NordVPN user flows",
  "main": "index.js",
  "scripts": {
    "test:ui": "playwright test tests/e2e/*.spec.ts",
    "test:ui:headed": "playwright test tests/e2e/*.spec.ts --headed",
    "report": "playwright show-report",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test:api": "playwright test tests/api/*.spec.ts"
  },
  "keywords": [
    "testing",
    "e2e",
    "playwright",
    "nordvpn"
  ],
  "author": "Micha≈Ç Czok",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^0.15.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.1.0",
    "typescript": "^5.0.0"
  }
}
